<h2>Вопросы для теста <%= @test.title %>:</h2>

<% if @questions.present? %>
  <table>
    <tr>
      <th>Question</th>
      <th>Test</th>
      <th colspan="3">Actions</th>
    </tr>
    <%= render @questions %>
  </table>
<% else %>
  <p>Ничего не найдено</p>
<% end %>

<%= link_to 'New question', new_test_question_path(@test) %> |
<%= link_to 'Back', tests_path %>
